
<%= form_for @user, url: login_path, method: :post do |f| %>
  <%= f.label :nick_name %>
  <%= f.text_field :nick_name %>

  <%= f.label :password_digest %>
  <%= f.password_field :password_digest %>

  <button><%= f.submit "log in" %></button>
  Or <%= link_to(controller: 'users', action: 'new') { 'sign up' } %>
<% end %>