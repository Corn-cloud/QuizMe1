
<h1>Sign Up</h1>
<p>Create a User to participate</p>




<%= form_for @user, url: signup_path, method: :post do |f| %>
  <%= f.label :nick_name %>
  <%= f.text_field :nick_name %>
  <br/>
  <%= f.label :email %>
  <%= f.text_label :email %>
  <br/>
 <%= f.label :password %>
 <%= f.password_field :password %>

  <%= f.submit "Sign up" %>
  Or <%= link_to @user, user_path %>
<% end %>